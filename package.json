{
  "name": "@pnpm/read-package-json",
  "version": "1.0.1",
  "description": "Read a package.json",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "tslint --config tslint.json --project . && preview && ts-node test --type-check",
    "prepublishOnly": "tsc",
    "tsc": "tsc",
    "md": "mos"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnpm/read-package-json.git"
  },
  "keywords": [
    "pnpm",
    "outdated"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "z@kochan.io",
    "url": "https://www.kochan.io/",
    "twitter": "ZoltanKochan"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/read-package-json/issues"
  },
  "homepage": "https://github.com/pnpm/read-package-json#readme",
  "dependencies": {
    "@pnpm/types": "^2.0.0",
    "read-package-json": "2.0.13",
    "util.promisify": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.3.2",
    "@types/tape": "^4.2.31",
    "mos": "^2.0.0-alpha.3",
    "mos-plugin-readme": "^1.0.4",
    "package-preview": "^2.0.0",
    "tape": "^4.8.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.8.0",
    "typescript": "^3.0.0"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  }
}
